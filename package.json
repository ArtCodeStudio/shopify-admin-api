        "test:webhooks:raw": "alsatian -T ./dist/tests/webhooks.js",
        "test:customers:raw": "alsatian -T ./dist/tests/customers.js",
        "test:data": "alsatian -T ./dist/tests/data.js | tap-spec",
        "test:data:raw": "alsatian -T ./dist/tests/data.js",
        "test:test_utils": "alsatian -T ./dist/tests/test_utils.js | tap-spec",
        "test:test_utils:raw": "alsatian -T ./dist/tests/test_utils.js"
    }
